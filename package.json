{
  "name": "remark-behead",
  "version": "1.5.3",
  "description": "Add or remove heading levels",
  "main": "dst/lib/index.js",
  "scripts": {
    "tape": "tape test/*.js",
    "babel": "babel src -d .",
    "posttest": "npm run coverage",
    "test": "npm run babel && nyc npm run tape",
    "prepublish": "npm t && npm run clean && npm run babel",
    "clean": "rimraf index.js test coverage .nyc_output coverage.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "readme": "mv .babelrc b; documentation readme src/lib/index.js --readme-file Readme.md -s \".\"; mv b .babelrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzmmr/remark-behead.git"
  },
  "keywords": [
    "mdast",
    "remark",
    "behead",
    "plugin",
    "mdast-plugin",
    "mdast-behead",
    "remark-plugin",
    "remark-behead"
  ],
  "author": "mrzmmr",
  "license": "ISC",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "dependencies": {
    "defop": "^1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "codecov": "^1.0.1",
    "documentation": "github:documentationjs/documentation",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.1.2",
    "remark": "^3.1.3",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3"
  }
}
