{
  "name": "remark-behead",
  "version": "2.1.0",
  "description": "Increase or decrease heading depth",
  "main": "index.js",
  "scripts": {
    "lint": "xo --fix src/*/*",
    "pretest": "npm run build",
    "prebuild": "npm run lint",
    "build": "babel src -d . --source-maps",
    "tap": "tap -Rspec test/*.js",
    "test": "nyc npm run tap",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf test index* coverage .nyc_output"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzmmr/remark-behead.git"
  },
  "keywords": [
    "mdast",
    "remark",
    "behead",
    "plugin",
    "mdast-plugin",
    "mdast-behead",
    "remark-plugin",
    "remark-behead"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "dependencies": {
    "unist-util-find": "^1.0.1",
    "unist-util-find-all-after": "^1.0.0",
    "unist-util-find-all-before": "^2.0.0",
    "unist-util-find-all-between": "^1.0.1",
    "unist-util-visit": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "remark": "^8.0.0",
    "rimraf": "^2.6.2",
    "tap": "^12.1.1",
    "xo": "^0.23.0"
  }
}
