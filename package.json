{
  "name": "remark-behead",
  "version": "1.5.5",
  "description": "Increase or decrease heading weights",
  "main": "index.js",
  "scripts": {
    "tape": "tape test/*.js",
    "babel": "babel src -d .",
    "posttest": "npm run coverage",
    "test": "npm run babel && nyc npm run tape",
    "prepublish": "npm t && npm run clean && npm run babel",
    "clean": "rimraf index.js test coverage .nyc_output coverage.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzmmr/remark-behead.git"
  },
  "keywords": [
    "mdast",
    "remark",
    "behead",
    "plugin",
    "mdast-plugin",
    "mdast-behead",
    "remark-plugin",
    "remark-behead"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "dependencies": {
    "lodash.defaultsdeep": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "codecov": "^1.0.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.1.2",
    "remark": "^3.1.3",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3"
  }
}
